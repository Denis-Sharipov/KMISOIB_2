<Window x:Class="KMiSOIB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KMiSOIB"
        mc:Ignorable="d"
        Title="КМиСОИБ" Height="180" Width="191">
    <Grid>
        <Button x:Name="RsaWindow_Button" Content="3) RSA" HorizontalAlignment="Left" Margin="10,62,0,0" VerticalAlignment="Top" Width="161" RenderTransformOrigin="0.053,1" Click="RsaWindow_Button_Click"/>
