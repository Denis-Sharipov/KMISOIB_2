        <Button x:Name="HashFunctionWindow_Button" Content="4) Функция хеширования" HorizontalAlignment="Left" Margin="10,87,0,0" VerticalAlignment="Top" Width="161" RenderTransformOrigin="0.053,1" Click="HashFunctionWindow_Button_Click"/>
        <Button x:Name="DsWindow_Button" Content="5) ЭЦП" HorizontalAlignment="Left" Margin="10,112,0,0" VerticalAlignment="Top" Width="161" RenderTransformOrigin="0.053,1" Click="DsWindow_Button_Click"/>

